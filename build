./clean
echo "compile face"
clang -o faceC.o -c face.c -I /usr/local/include/lua
touch face.hs
clang -o face.so -fPIC -shared faceC.o -llua
echo "link facer"
clang -o a.out facer.c faceC.o -llua
ghc -o b.out facer.hs faceC.o -llua
chmod +x facer.ex
echo "execute facer"
./a.out
./b.out
./facer.ex
echo "generage typer"
./typra.ex
echo "link typer"
clang -o a.out typer.c faceC.o -llua
ghc -o b.out typer.hs faceC.o -llua
chmod +x typer.ex
echo "execute typer"
./a.out
echo "generate type"
./type.ex
echo "compile type"
clang -o typeC.o -c type.c
touch type.hs
touch type.lua

echo "build done"
